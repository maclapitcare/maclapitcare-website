{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/test.js": {
      "runtime": "nodejs18.x"
    },
    "api/service-booking.js": {
      "runtime": "nodejs18.x"
    },
    "api/contact.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}