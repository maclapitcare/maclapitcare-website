{
  "version": 2,
  "framework": null,
  "buildCommand": "npx vite build",
  "outputDirectory": "dist",
  "installCommand": "npm install",

  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}